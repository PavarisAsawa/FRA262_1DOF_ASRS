#include "main.h"
#include "Trajectory.h"

void QuinticTrajectory_Init(TrajectoryTypeDef *Traject ,QEIStructureTypeDef *QEI ,float FinalPos,float v_max)
{
	Traject->InitialPosition = QEI->LinearPosition;
	Traject->FinalPosition = FinalPos;
	Traject->MaximumVelocity = v_max;
}

void QuinticTrajectory_Generation(TrajectoryTypeDef *Trajectory , float initial_pos , float final_pos)
{
	if(initial_pos != final_pos)
	{
		float distance = final_pos - initial_pos;
//		static float a0 =
	}
}

